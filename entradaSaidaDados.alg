algoritmo "Entrada e saida de dados"
// Função :
// Autor :
// Data : 5/27/2020
// Seção de Declarações 
var
   nome: caractere
   nota: inteiro
inicio
// Seção de Comandos
   escreval("Digite o seu nome: ")
   leia(nome)
   escreval("Digite a sua idade: ")
   leia(nota)
   
   escreval("O seu nome é " ,nome)
   escreval("A sua idade é " ,nota)
fimalgoritmo
